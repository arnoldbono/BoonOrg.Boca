{
  "version": "2.0.0",
  "tasks": [
      {
      "label": "build Boca",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/src/Boca.csproj",
        "-c",
        "Debug",
        "-p:Platform=arm64",
        "-o",
        "${workspaceFolder}/Output",
        "-f:net8.0"
      ],
      "problemMatcher": "$msCompile",
      "dependsOn": []
    },
    {
      "label": "build",
      "dependsOn": [
        "build Boca"
      ],
      "problemMatcher": []
    }
  ],
  "options": {
    "continueOnError": "false"
  }
}